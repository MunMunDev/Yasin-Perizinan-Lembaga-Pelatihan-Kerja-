<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tvNo"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="NO"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:gravity="center"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvNamaUser"
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:text="Nama User"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvNamaPelatihan"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:text="Nama Pelatihan"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

<!--    <TextView-->
<!--        android:id="@+id/tvJenisDokumen"-->
<!--        android:layout_width="200dp"-->
<!--        android:layout_height="60dp"-->
<!--        android:text="Jenis Dokumen (KTP, dll)"-->
<!--        android:textSize="14sp"-->
<!--        android:textColor="@color/black"-->
<!--        android:textStyle="normal"-->
<!--        android:gravity="center"-->
<!--        android:lines="1"-->
<!--        android:ellipsize="end"-->
<!--        android:background="@drawable/bg_table"/>-->

    <TextView
        android:id="@+id/tvTanggal"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:text="Tanggal"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvWaktu"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:text="Waktu"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvEkstensi"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:text="Ekstensi"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvFile"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:text="File"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvDokumen"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:text="Dokumen"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvVerifikasi"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:text="Verivikasi"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvCatatan"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:text="Verivikasi"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:gravity="center"
        android:lines="1"
        android:ellipsize="end"
        android:background="@drawable/bg_table"/>

    <TextView
        android:id="@+id/tvSetting"
        android:layout_width="40dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="0dp"
        android:text=":::"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:gravity="center"
        android:background="@drawable/bg_table" />

</LinearLayout>